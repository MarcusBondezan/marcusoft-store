generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id            Int       @id @default(autoincrement())
  description   String
  price         Decimal
  width         Int       
  height        Int
  length        Int
  weight        Decimal

  @@map("product")
}

model Coupon {
  code            String    @id
  percentage      Decimal
  expire_date     DateTime

  @@map("coupon")
}